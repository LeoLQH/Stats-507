    "def add_border(x):\n",
    "    dimen = np.shape(x)\n",
    "    dim_zeros = np.shape(x)[0] + 2\n",
    "    dim_ones = np.shape(x)[1] + 2\n",
    "    zeros_arr = np.zeros((dim_zeros, dim_ones))\n",
    "    zeros_arr[1:(np.shape(x)[0] + 1), 1:(np.shape(x)[1] + 1)] = x\n",
    "    return zeros_arr\n",
    "    # raise NotImplementedError()"
   ]
