    "def make_checkerboard(n):\n",
    "    one_dim = np.zeros((n, n))\n",
    "    for i in range(n): \n",
    "        if i % 2 == 0: \n",
    "            where_one = np.arange(1, n, 2) \n",
    "        else: \n",
    "            where_one = np.arange(0, n, 2) \n",
    "        one_dim[i, where_one] = 1\n",
    "    return one_dim\n",
    "    # YOUR CODE HERE\n",
